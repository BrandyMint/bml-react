  "description": "Awesome BML front-end",
  "license": "UPL-1.0",
  "scripts": {
    "build": "npm run build:production:viewer && npm run build:production:editor && npm run build:prerender",
    "build:production:viewer": "NODE_ENV=production node -r babel-core/register ./node_modules/.bin/webpack --profile --config ./webpack/production-viewer.js",
    "build:production:editor": "NODE_ENV=production node -r babel-core/register ./node_modules/.bin/webpack --profile --config ./webpack/production-editor.js",
    "build:prerender": "NODE_ENV=production node -r babel-core/register ./node_modules/.bin/webpack --profile --config ./webpack/prerender.js",
    "start": "node -r babel-register ./initializers/server/index.js",
    "lint": "eslint --ext .json --ext .js --ext .jsx src initializers test",
    "fix": "eslint --fix --ext .js --ext .jsx src initializers test",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "screenshot": "node ./screenshoter.js http://localhost:3000/preview 960x720 src/assets/images/screenshot.png",
    "bump": "npm version patch && npm run build && git add . && git commit -m build && git push",
    "deploy": "rm -fr ./ghpages; mkdir ./ghpages; cp -vr ./src/assets ./dist-editor/* ./ghpages; rm ./ghpages/stats.json; git add ghpages; git commit -m 'publicate ghpages',:tabe./node_modules/ghpages/cli.js -d yopage.ru -p ghpages"
  },
